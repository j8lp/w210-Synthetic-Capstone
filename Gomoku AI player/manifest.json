{
    "manifest_version": 2,
      "browser_action": {
    "default_title" : "Gomoku AI",
    "default_popup": "popup_inactive.html"
  },

"icons": { "48": "icons/icon_48.png",
          "128": "icons/icon_128.png" },

    "content_scripts": [ {
        "exclude_globs":    [  ],
        "include_globs":    [ "*" ],
        "js":               [  "scripts/Gomoku AI player.user.js" ],
        "matches":          [  "https://*.boardgamearena.com/gomoku*"],
        "run_at": "document_end"
    } ],
    "converted_from_user_script": true,
    "description":  "Gomoku AI for Board Game Arena",
    "name":         "/Gomoku AI player",
    "version":      "0.2"
}
